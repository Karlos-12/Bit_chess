<Window x:Class="Bitchess.resultscreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bitchess"
        mc:Ignorable="d"
        Title="Results" Height="450" Width="800" Foreground="{x:Null}" WindowStartupLocation="CenterScreen" Topmost="True">
    <Window.Background>
        <ImageBrush ImageSource="/Bez dfgdnázvu.png"/>
    </Window.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="109*"/>
            <RowDefinition Height="108*"/>
        </Grid.RowDefinitions>
        <Label x:Name="splash" Content="SAmole" Margin="0,61,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="{x:Null}" Foreground="#FF023E00" FontSize="90" FontFamily="Consolas"/>
        <Button Content="Continue" Margin="131,33,134,32" Background="{x:Null}" BorderBrush="#FF023E00" Foreground="#FF023E00" FontSize="64" FontFamily="Consolas" FontWeight="Bold" BorderThickness="5,5,5,5" Grid.Row="1" Click="Button_Click"/>

    </Grid>
</Window>
